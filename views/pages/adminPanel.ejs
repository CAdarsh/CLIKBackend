        <form class="new-entry-form">
          <div class="text-inp-cont ">
            <label class="name-l field-label">Company Name</label>
            <input class="text-inp cn" type="text" required />
            <div class="feed-text"></div>
          </div>
          <div class="text-inp-cont ">
            <label class="phone-l field-label">Phone Number</label>
            <input class="text-inp pn" type="text" required />
            <div class="feed-text"></div>
          </div>
          <div class="text-inp-cont ">
