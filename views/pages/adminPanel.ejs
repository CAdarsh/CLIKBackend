            <div class="feed-text"></div>
          </div>
          <div class="text-inp-cont slug-inp-cont">
            <label class="slug-l field-label">Slug (clikindia.co.in/member/[slug])</label>
            <input class="text-inp sl" type="text" required />
            <div class="feed-text"></div>
          </div>
          <div class="feed-text-error"></div>
  
